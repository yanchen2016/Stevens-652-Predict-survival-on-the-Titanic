#import packages
import numpy as np
import pandas as pd
import sklearn.linear_model as lm
from sklearn.model_selection import GridSearchCV
import matplotlib.pyplot as plt
%matplotlib inline

#read train data
train = pd.read_csv('/Users/yan/Documents/652C-Hu Yifan/final project/Titanic/train.csv')
train

#Replace the missing value of Age with median
train['Age'] = train['Age'].fillna(train['Age'].median())

#Replace the missing value of Embarked with C
train['Embarked'] = train['Embarked'].fillna('C')

#Replace the missing value of Cabin with U0
train['Cabin'] = train['Cabin'].fillna('U0')
train
